				<xsd:annotation>
					<xsd:documentation>Наименование на русском языке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NameKz" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование на государственном языке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
